        <activity android:name=".views.activitysEntrada.Entrada1" />
        <activity android:name=".views.InicioActivity" />
        <activity
            android:name=".views.administrar.ModuloComunicacionActivity"
            android:parentActivityName=".views.InicioActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.tdt.artlux.views.InicioActivity" />
        </activity>
        <activity
            android:name=".views.administrar.LicenciaActivity"
            android:parentActivityName=".views.InicioActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.tdt.artlux.views.InicioActivity" />
        </activity>
    </application>

</manifest>