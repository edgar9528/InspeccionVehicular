        android:theme="@style/AppTheme">
        <activity android:name=".views.PrincipalActivity">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>


        <activity android:name=".views.activitysSalida.Salida2" />
        <activity android:name=".views.activitysSalida.Salida1" />
        <activity android:name=".views.activitysEntrada.Entrada5" />
        <activity android:name=".views.activitysEntrada.Entrada4" />
        <activity android:name=".views.activitysEntrada.Entrada3">
            android:parentActivityName=".views.activitysEntrada.Entrada2">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".views.activitysEntrada.Entrada2" />
        </activity>
        <activity android:name=".views.activitysEntrada.Entrada2">
            android:parentActivityName=".views.activitysEntrada.Entrada1">
